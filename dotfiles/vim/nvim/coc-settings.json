{
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "initializationOptions": {
         "cacheDirectory": "/tmp/ccls"
       }
    }
  },

  "python.jediEnabled": false,

  "coc.preferences.diagnostic.displayByAle": true,
}
